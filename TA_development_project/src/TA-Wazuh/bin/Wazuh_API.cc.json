{
    "tokens": [
        "__settings__.logging.loglevel", 
        "global_account.username", 
        "global_account.password"
    ], 
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    }, 
    "meta": {
        "apiVersion": "1.0.0"
    }, 
    "requests": [
        {
            "post_process": {
                "pipeline": [
                    {
                        "output": "__stdout__", 
                        "input": [
                            "{{__response__.body}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ], 
                        "method": "splunk_xml"
                    }, 
                    {
                        "input": [
                            "{{__stdout__}}"
                        ], 
                        "method": "std_output"
                    }
                ], 
                "skip_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}", 
                            "$"
                        ], 
                        "method": "json_empty"
                    }
                ]
            }, 
            "iteration_mode": {
                "iteration_count": "1", 
                "stop_conditions": []
            }, 
            "pre_process": {
                "pipeline": [], 
                "skip_conditions": []
            }, 
            "request": {
                "url": "http://10.0.0.51:55000/agents", 
                "headers": {}, 
                "method": "GET", 
                "auth": {
                    "type": "basic_auth", 
                    "options": {
                        "password": "{{global_account.password}}", 
                        "username": "{{global_account.username}}"
                    }
                }
            }
        }
    ]
}