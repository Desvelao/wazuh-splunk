{
    "meta": {
        "apiVersion": "1.0.0"
    }, 
    "tokens": [
        "__settings__.logging.loglevel", 
        "global_account.username", 
        "global_account.password"
    ], 
    "requests": [
        {
            "pre_process": {
                "skip_conditions": [], 
                "pipeline": []
            }, 
            "request": {
                "url": "http://10.0.0.51:55000/rules", 
                "method": "GET", 
                "auth": {
                    "type": "basic_auth", 
                    "options": {
                        "username": "{{global_account.username}}", 
                        "password": "{{global_account.password}}"
                    }
                }, 
                "headers": {}
            }, 
            "iteration_mode": {
                "stop_conditions": [], 
                "iteration_count": "1"
            }, 
            "post_process": {
                "skip_conditions": [
                    {
                        "method": "json_empty", 
                        "input": [
                            "{{__response__.body}}", 
                            "$"
                        ]
                    }
                ], 
                "pipeline": [
                    {
                        "output": "__stdout__", 
                        "method": "splunk_xml", 
                        "input": [
                            "{{__response__.body}}", 
                            "", 
                            "{{index}}", 
                            "{{host}}", 
                            "{{source}}", 
                            "{{sourcetype}}"
                        ]
                    }, 
                    {
                        "method": "std_output", 
                        "input": [
                            "{{__stdout__}}"
                        ]
                    }
                ]
            }
        }
    ], 
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    }
}