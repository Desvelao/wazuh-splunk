  <row>
    <panel>
      <title>Top agent</title>
      <single>
        <search>
          <query>index=wazuh sourcetype="wazuh" rule.groups="syscheck" "agent.name"="*" | top "agent.name" showcount=false showperc=false |head 1</query>
